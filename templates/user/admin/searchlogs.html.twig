{% extends 'layouts/base.html.twig' %}

{% block title %}Terminos de busqueda {% endblock %}

{% block main %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 1200px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
<div class="example-wrapper">
<a href=" {{ path('home')}}"><button class="btn btn-primary">volver</button></a>
			
<!-- Hoverable rows -->
<table class="table table-hover text-center">
  <thead>
    <tr>
      <th>FECHA Y TÉRMINOS DE BUSQUEDA EN LA PÁGINA (datos sacados del var/searchlog no de la DB)</th>
    </tr>
  </thead>
  <tbody>
  {% if resultado %}
  	{% for log in resultado %}
        <tr>
          <th>{{ log }}</th>
        </tr>
	  {% endfor %}
  {% else %}
    <th>Aún no hay términos que hayan buscado los usuarios </th>
  {% endif %}
  </tbody>
</table>

		
{% endblock %}